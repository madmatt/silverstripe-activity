{
    "name": "madmatt/silverstripe-activity",
    "type": "silverstripe-vendormodule",
    "description": "Provides an activity feed for SilverStripe members",
    "homepage": "https://github.com/madmatt/silverstripe-activity",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "activity",
        "logs"
    ],
    "authors": [
        {
            "name": "madmatt",
            "homepage": "https://github.com/madmatt"
        }
    ],
    "require": {
        "composer/installers": "*",
        "silverstripe/admin": "^1",
        "silverstripe/framework": "^4",
        "silverstripe/vendor-plugin": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "autoload": {
        "psr-4": {
            "Madmatt\\ActivityLog\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}